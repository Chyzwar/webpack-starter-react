[ignore]
.*/build/*
.*/covreage/*
.*/node_modules/*

[untyped]
.*/src/config/*
.*/src/store/*
.*/src/reducers/*
.*/src/containers/*
.*/src/main.js

[libs]
./flow-typed/*

[options]
all=true
module.name_mapper='^actions\(\/?.*\)$' -> '<PROJECT_ROOT>/src/actions/\1'
module.name_mapper='^components\(\/?.*\)$' -> '<PROJECT_ROOT>/src/components/\1'
